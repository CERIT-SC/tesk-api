{
  "kind": "Job",
  "apiVersion": "batch/v1",
  "metadata": {
    "name": "taskmaster"
  },
  "spec": {
    "template": {
      "metadata": {
        "name": "taskmaster"
      },
      "spec": {
        "containers": [
          {
            "name": "taskmaster",
            "image": "eu.gcr.io/tes-wes/taskmaster:v0.0.5.2",
            "args": [
              "$(JSON_INPUT)"
            ],
            "env": [
              {
                "name": "JSON_INPUT",
                "value": ""
              }
            ]
          }
        ],
        "restartPolicy": "Never"
      }
    }
  }
}
